   "source": [
    "cnt = int(input(\"학생 수:\"))\n",
    "n=cnt\n",
    "p=cnt\n",
    "scores=[]\n",
    "while cnt>0:\n",
    "    x= input(\"성적 : \")\n",
    "    scores.append(int(x))\n",
    "    cnt-=1\n",
    "    \n",
    "def find_max(scores):\n",
    "    max_val = scores[0]\n",
    "    for i in scores:\n",
    "        if i > max_val:\n",
    "            max_val = i\n",
    "    return max_val\n",
    "\n",
    "print(find_max(scores))"
   ]
